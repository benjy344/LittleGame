<%= simple_form_for(@room) do |f| %>
  	<%= f.error_notification %>

  	<div class="form-inputs">
	    <%= f.input :name, label:'Nom', class: 'form-control' %>
	    <%= f.input :donjon_ids, input_html: { value: @donjon.id }, label:'Nom', class: 'form-control', as: :hidden %>
	  </div>

  	<div class="form-actions">
    	<%= f.button :submit %>
  	</div>



<% end %>
